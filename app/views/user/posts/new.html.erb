<%= form_with(model: @post, local: true) do |f| %>

  <div>
    <%= f.label :name, '名前' %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :images, '画像' %>
    <%= f.file_field :images, multiple: true %>
  </div>
  
  <p class="text-justify"></p>

  <div>
    <%= f.submit  '投稿する' %>
  </div>
<% end %>